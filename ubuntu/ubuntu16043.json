{
  "builders":
  [
    {
      "type": "qemu",
      "iso_url": "http://10.11.1.21/isos/CentOS-7-x86_64-Minimal-1708.iso",
      "iso_checksum_type": "md5",
      "iso_checksum": "10fcd20619dce11fe094e960c85ba4a9",
      "output_directory": "output_m",
      "shutdown_command": "sudo systemctl poweroff",
      "disk_size": 40960,
      "format": "qcow2",
      "headless": false,
      "accelerator": "kvm",
      "http_directory": "httpdir",
      "ssh_port": 22,
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_wait_timeout": "10m",
      "vm_name": "ubuntu16043build",
      "net_device": "virtio-net",
      "disk_interface": "virtio",
      "qemu_binary": "qemu-kvm",
      "qemuargs": [
        ["-m", "2048"],
        ["-smp", "2"]
      ],
      "vnc_bind_address": "0.0.0.0",
      "boot_wait": "5s",
      "boot_command": [
        "<enter><wait>",
        "<f6><esc>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs><bs><bs><bs><bs><bs><bs><bs>",
        "<bs><bs><bs>",
        "/install/vmlinuz ",
        "initrd=/install/initrd.gz ",
        "net.ifnames=0 ",
        "auto-install/enable=true ",
        "debconf/priority=critical ",
        "preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ubuntu-16.04/preseed.cfg ",
        "<enter>"
      ]
    }
  ],
  "post-processors": [{
    "type": "vagrant",
    "compression_level": "6",
    "output": "ubuntu16043x86_64.qcow2"
  }],
  "provisioners": [{
    "type": "shell",
    "scripts": [
      "scripts/sshd.sh",
      "scripts/cleanup.sh",
      "scripts/minimize.sh"
    ]
  }]
}